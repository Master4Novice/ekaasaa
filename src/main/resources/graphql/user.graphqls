input User {
    firstName: String!
    middleName: String
    lastName: String!
    incomes: [Income!]
}

type UserData {
    id: String!
    firstName: String
    middleName: String
    lastName: String
    incomes: [IncomeData!]
}

type Query {
    getUser(id : ID!): UserData
    getUsers: [UserData]
}

type Mutation {
    putUser(user: User!): String
    updateUser(id: ID!, user: User): UserData
    deleteUser(id: ID!): String
}

type Subscription {
    userSub: [UserData]
}